ass=\"input-field col s12\">\n                        <label class=\"active\">상세 내용</label>\n                        <input type=\"text\" id=\"student-lesson-about\">\n                    </div>\n                    <br>\n                </div>\n            </div>\n            <div id=\"append-student\">\n            </div>\n            <div id=\"for-append\" class=\"\">\n                <div class=\"input-field col s10\">\n                    <SelectForm class=\"input-field col s6\" v-bind:selectList=\"getStudents\" v-on:input=\"onChangeStudent\" name=\"학생\"></SelectForm>\n                </div>\n                <button class=\"btn waves-effect waves-light\" id=\"append-btn\" onclick=\"append_student()\">학생 추가</button>\n            </div>\n        </div>\n        <button class=\"btn waves-effect waves-light\" id=\"add-teacherdiary-btn\" onclick=\"add_teacher_diary()\"\n                name=\"action\">일지추가\n            <i class=\"material-icons right\">send</i>\n        </button>\n    </div>\n</template>\n\n\n<script>\n    import TeacherService from '../../service/teachers.js';\n    import SelectForm from '../form/SelectForm.vue';\n    import Date from '../form/Date.vue';\n    import Time from '../form/Time.vue';\n    import Utility from '../../Utility/Utility.js';\n\n    export default {\n        name: 'TeacherInformation',\n        data() {\n            return {\n                Teacher: {\n                    name: '',\n                    department: '',\n                    phone: '',\n                },\n                ModifyingTeacher: false,\n                CompletedDiarys: [],\n                noneCompletedDiarys: [],\n                Diary: {\n                    lesson_type: null,\n                    lesson_time: {\n                        date: Utility.getDateBaseNow(0, 0, 0),\n                        start: Utility.getTimeBaseNow(-3, 0),\n                        end: Utility.getTimeBaseNow(0, 0),\n                    },\n                },\n         