urces":["teacher_information.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAoGA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"teacher_information.vue","sourceRoot":"src/components/teachers","sourcesContent":["<div class=\"container\">\n    <div class=\"row\">\n        <div class=\"p-40 border r-3 white\" style=\"\">\n            <br/>\n            <h4><%= teacher.name %></h4>\n            <br/>\n            <div class=\"row\">\n                <input hidden type=\"text\" value=\"<%= teacher._id %>\" id=\"teacher-id\">\n                <div class=\"col-12\">\n                    <label for=\"department\" class=\"required\">소속</label>\n                    <input class=\"form-control form-control-lg\" type=\"text\" name=\"department\"\n                           value=\"<%= teacher.department %>\"\n                           id=\"department\" placeholder=\"\" disabled>\n                </div>\n                <div class=\"col-12\">\n                    <label for=\"phone\" class=\"required\">전화번호</label>\n                    <input class=\"form-control form-control-lg\" type=\"text\" name=\"phone\"\n                           value=\"<%= teacher.phone %>\"\n                           id=\"phone\" placeholder=\"\" disabled>\n                </div>\n            </div>\n            <div class=\"text-center p-t-20\">\n                <button class=\"btn\" onclick=\"modify_teacher();\" id=\"change-btn\"><i\n                            class=\"icon icon-pencil\"></i> 수정하기\n                </button>\n                <button hidden class=\"btn hide\" onclick=\"do_modify_teacher();\" id=\"do-chang-btn\"><i\n                            class=\"icon icon-pencil\"></i>완료\n                </button>\n            </div>\n        </div>\n    </div>\n</div>\n\n<div class=\"container\">\n    <div class=\"row\">\n        <div class=\"input-field col s4\">\n            <label id=\"now-date-label\" class=\"active\">수업시간</label>\n            <input type=\"text\" class=\"datepicker\" id=\"now-date\" required value=\"\">\n        </div>\n        <div class=\"input-field col s4\">\n            <label id=\"start-time-label\" class=\"active\">시작 시간</label>\n            <input type=\"text\" class=\"timepicker\" id=\"start-time\" required>\n        </div>\n        <div class=\"input-field col s4\">\n            <label id=\"end-time-label\" class=\"active\">종료 시간</label>\n            <input type=\"text\" class=\"timepicker\" id=\"end-time\" required>\n        </div>\n        <div class=\"input-field col s12\">\n            <label class=\"active\">수업 종류</label>\n            <select id=\"lesson-type\">\n                <% lessons.forEach(function (lesson) { %>\n                    <option value=\"<%= lesson._id %>\"><%= lesson.type %></option>\n                <% }) %>\n            </select>\n        </div>\n    </div>\n    <br/>\n    <div class=\"row\" id=\"form-student\">\n        <div class=\"hide\">\n            <div id=\"form\">\n                <div class=\"input-field col s4\">\n                    <label id=\"student-name-label\" class=\"active\">이름</label>\n                    <input type=\"text\" id=\"student-id\" disabled>\n                </div>\n                <div class=\"input-field col s4\">\n                    <label id=\"start-time-label\" class=\"active\">시작 시간</label>\n                    <input type=\"text\" id=\"student-start-time\" class=\"timepicker\">\n                </div>\n                <div class=\"input-field col s4\">\n                    <label id=\"end-time-label\" class=\"active\">종료 시간</label>\n                    <input type=\"text\" id=\"student-end-time\" class=\"timepicker\">\n                </div>\n                <div class=\"input-field col s12\">\n                    <label class=\"active\">상세 내용</label>\n                    <input type=\"text\" id=\"student-lesson-about\">\n                </div>\n                <br>\n            </div>\n        </div>\n        <div id=\"append-student\">\n        </div>\n        <div id=\"for-append\" class=\"\">\n            <div class=\"input-field col s10\">\n                <label class=\"active\">학생</label>\n                <select id=\"append-student-id\">\n                    <% students.forEach(function(student) { %>\n                        <option value=\"<%= student._id %>\"><%= student.name %></option>\n                    <% }) %>\n                </select>\n            </div>\n            <button class=\"btn waves-effect waves-light\" id=\"append-btn\" onclick=\"append_student()\">학생 추가</button>\n        </div>\n    </div>\n    <button class=\"btn waves-effect waves-light\" id=\"add-teacherdiary-btn\" onclick=\"add_teacher_diary()\"\n            name=\"action\">일지추가\n        <i class=\"material-icons right\">send</i>\n    </button>\n</div>\n\n<script>\n    function append_student() {\n        let StudentId = $('#append-student-id').val();\n        let form = $('#form').html();\n        let append = \"<div id='\" + StudentId + \"'>\" + form + \"</div>\";\n        $('#append-student').append(append);\n        $('#' + StudentId).find('#student-name-label').attr('class', 'active');\n        $('#' + StudentId).find('#start-time-label').attr('class', 'active');\n        $('#' + StudentId).find('#end-time-label').attr('class', 'active');\n        $('#' + StudentId).find('#student-id').attr('value', $('#append-student-id option:selected').text());\n        $('#' + StudentId).find('#student-start-time').attr('value', $('#start-time').val());\n        $('#' + StudentId).find('#student-end-time').attr('value', $('#end-time').val());\n        $('#append-student-id option:selected').remove();\n    }\n</script>"]}]}